function onloadevent() {
    let array1 = [["1","2","3","4","5","6","40","31", "9", "10", "45"],
                  ["38","13","14","15","16","17","18", "19", "20", "42", "43"],
                  ["21","7","22","23","24","25","26","27", "28", "29", "30"],
                  ["8","32","33","34","35","36","37","11", "39", "41", "44"]];
    for (row=0; row<4; row++) {
        for (i of array1[row]) {
            document.getElementById("row"+(row+1)).innerHTML += "<img src='srv/pics/"+i+".jpg' width='100%'>";
        }
    }
}
function onloadeventBuddies() {
    var cnt = 1;
    for (row=0; row<4; row++) {
        for (i=1;i<=11;i++) {
            document.getElementById("brow"+(row+1)).innerHTML += "<img src='buddies/"+(cnt)+".jpg' width='100%'>";
            cnt += 1;
        }
    }
}
